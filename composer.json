{
    "name": "benoit.mallo/move-city",
    "authors": [
        {
            "name": "Beno√Æt Mallo",
            "email": "benoit.mallo@bappli.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "psr/container": "^1.0",
        "geo6/geocoder-php-addok-provider": "^1.0",
        "geocoder-php/google-maps-provider": "^4.2",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message": "^1.7",
        "symfony/http-foundation": "^4.1",
        "doctrine/dbal": "^2.8",
        "tuxboy/sdam": "^1.0",
        "symfony/console": "^4.1",
        "doctrine/collections": "^1.5",
        "intervention/image": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Core\\": "core/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/debug": "^4.1",
        "symfony/var-dumper": "^4.1",
        "phpunit/phpunit": "^7.4",
        "satooshi/php-coveralls": "^1.0"
    },
    "scripts": {
        "start": "php -S localhost:8000 -t public/ -ddisplay_errors=1",
        "test": "phpunit"
    }
}
